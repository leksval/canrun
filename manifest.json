{
  "name": "CanRun Gaming Compatibility Checker",
  "version": "1.0.0",
  "manifestVersion": 1,
  "executable": "./g-assist-plugin-canrun.exe",
  "persistent": true,
  "functions": [
    {
      "name": "canrun",
      "description": "Check if your system can run games with S-A-B-C-D-F tier rating and detailed compatibility analysis",
      "tags": [
        "canrun",
        "compatibility",
        "gaming"
      ],
      "properties": {
        "game_name": {
          "type": "string",
          "description": "The name of the game to check compatibility for"
        }
      }
    },
    {
      "name": "initialize",
      "description": "Initialize the CanRun plugin"
    },
    {
      "name": "shutdown",
      "description": "Shutdown the CanRun plugin"
    }
  ],
  "nvidia_requirements": {
    "min_driver": "472.12",
    "gpu_series": ["RTX", "GTX"],
    "os": ["Windows 10", "Windows 11"]
  }
}